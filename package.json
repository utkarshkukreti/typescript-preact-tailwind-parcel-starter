{
  "scripts": {
    "start": "parcel --no-cache src/index.html src/index.tsx src/index.css",
    "build": "run-s clean build:parcel",
    "build:parcel": "cross-env PURGE=true parcel build --no-source-maps --no-cache src/index.html src/index.tsx src/index.css",
    "clean": "rm -rf .parcel-cache dist"
  },
  "dependencies": {
    "preact": "^10.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.4",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-nightly.148",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.8.3"
  }
}
