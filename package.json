{
  "scripts": {
    "start": "parcel --no-cache src/index.html src/index.tsx src/index.css",
    "build": "run-s clean build:parcel",
    "build:parcel": "cross-env PURGE=true parcel build --no-source-maps --no-cache src/index.html src/index.tsx src/index.css",
    "clean": "rm -rf .parcel-cache dist"
  },
  "dependencies": {
    "preact": "^10.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-react-jsx": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@tailwindcss/ui": "^0.3.0",
    "autoprefixer": "^9.8.4",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-nightly.314",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.5"
  }
}
